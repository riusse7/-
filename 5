from math import log10
a = int(input("Введите число a: "))
b = int(input("Введите число b: "))
print(a, "+", b, "=", a + b)
print(a, "-", b, "=", a - b)
print(a, "*", b, "=", a * b)
print(a, "/", b, "=", a / b)
print(a, "%", b, "=", a % b)
print("Десятичный логорифм числа", a, "равен", log10(a))
print(a, "в степени", b, "равно", a ** b)
