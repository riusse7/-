a = 200
b = 100
c = 25
d =10
e =5
x = int(input("Введите сумму в центах: "))
print(x // a, "двухдолларовых монет")
x = x % a
print(x // b, "однодолларовых монет")
x = x % b
print(x // c, "25–центовых монет")
x = x % c
print(x // d, "10–центовых монет")
x = x % d
print(x // e, "5–центовых монет")
x = x % e
print(x, "центов")
